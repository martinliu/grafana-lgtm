<!-- /opt/homebrew/etc/clickhouse-server/config.d/prometheus.xml -->
<clickhouse>
  <prometheus>
    <endpoint>/metrics</endpoint>
    <port>9363</port>
    <metrics>true</metrics>
    <events>true</events>
    <asynchronous_metrics>true</asynchronous_metrics>
    <status_info>true</status_info>
  </prometheus>
</clickhouse>
